RepeatProto = require "./proto"
RepeatClosure = require "./proto"

Benchmark = require("benchmark").Benchmark

suite = new Benchmark.Suite;

# add tests
suite.add('Repeat w/prototype', ->
  new RepeatProto(->).every(2, 'ms').for(20, 'ms').start()
)
# add listeners
.on('cycle', (event)->
  console.log(String(event.target));
)
.on('complete', ->
  console.log('Fastest is ' + this.filter('fastest').pluck('name'));
)
# run async
.run('async': true );